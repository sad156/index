<view style="width:100%;"><view class="uni-padding-wrap"><view class="page-section swiper"><view class="page-section-spacing"><swiper class="swiper" indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" circular="{{true}}"><swiper-item style="background:red;"><image class="swiper-item" style="width:100%;" src="../../static/img/static/image1.jpg" mode="aspectFill"></image></swiper-item><swiper-item><image class="swiper-item" style="width:100%;" src="../../static/img/static/image2.jpg" mode="aspectFill"></image></swiper-item><swiper-item><image class="swiper-item" style="width:100%;" src="../../static/img/static/image3.jpg" mode="aspectFill"></image></swiper-item></swiper></view></view></view><uni-notice-bar vue-id="1" show-icon="true" scrollable="true" single="true" text="[单行] 这是 NoticeBar 通告栏，这是 NoticeBar 通告栏，这是 NoticeBar 通告栏" onVueInit="__l"></uni-notice-bar><navigator url="../button/button"><button style="background:red;" type="primary" value="1">点击跳转</button></navigator><uni-grid vue-id="2" column="{{3}}" highlight="{{true}}" data-event-opts="{{[['^change',[['change']]]]}}" onChange="__e" onVueInit="__l"><block a:for="{{list}}" a:for-item="item" a:for-index="index" a:key="index"><uni-grid-item vue-id="{{('3-'+index)+','+('2')}}" index="{{index}}" onVueInit="__l"><view class="grid-item-box"><text class="text">{{item.text}}</text></view></uni-grid-item></block></uni-grid><uni-load-more vue-id="4" onVueInit="__l"></uni-load-more></view>